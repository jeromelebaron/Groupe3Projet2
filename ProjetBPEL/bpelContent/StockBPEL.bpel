<bpel:process name="StockBPEL"
         targetNamespace="http://stock.bpel.s2re.fr"
         suppressJoinFailure="yes"
         xmlns:tns="http://stock.bpel.s2re.fr"
         xmlns:bpel="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
         xmlns:nsproduit="http://webservice.stock.s2re.fr">

    <!-- Import the client WSDL -->
    <bpel:import namespace="http://webservice.stock.s2re.fr" location="stockWS.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"></bpel:import>
    <bpel:import location="StockBPELArtifacts.wsdl" namespace="http://stock.bpel.s2re.fr" 
	        importType="http://schemas.xmlsoap.org/wsdl/" />
         
    <!-- ================================================================= -->         
    <!-- PARTNERLINKS                                                      -->
    <!-- List of services participating in this BPEL process               -->
    <!-- ================================================================= -->         
    <bpel:partnerLinks>
        <!-- The 'client' role represents the requester of this service. -->
        <bpel:partnerLink name="client"
                     partnerLinkType="tns:StockBPEL"
                     myRole="StockBPELProvider"
                     />
        <bpel:partnerLink name="PartnerLinkStock" partnerLinkType="tns:PLStock" partnerRole="PLRoleStock"></bpel:partnerLink>
    </bpel:partnerLinks>
  
    <!-- ================================================================= -->         
    <!-- VARIABLES                                                         -->
    <!-- List of messages and XML documents used within this BPEL process  -->
    <!-- ================================================================= -->         
    <bpel:variables>
        <!-- Reference to the message passed as input during initiation -->
        <bpel:variable name="input"
                  messageType="tns:StockBPELRequestMessage"/>
                  
        <!-- 
          Reference to the message that will be returned to the requester
          -->
        <bpel:variable name="output"
                  messageType="tns:StockBPELResponseMessage"/>
        <bpel:variable name="produit" element="nsproduit:produitDto"></bpel:variable>
    </bpel:variables>

    <!-- ================================================================= -->         
    <!-- ORCHESTRATION LOGIC                                               -->
    <!-- Set of activities coordinating the flow of messages across the    -->
    <!-- services integrated within this business process                  -->
    <!-- ================================================================= -->         
    <bpel:sequence name="main">
        
        <!-- Receive input from requester. 
             Note: This maps to operation defined in StockBPEL.wsdl 
             -->
        <bpel:receive name="receiveInput" partnerLink="client"
                 portType="tns:StockBPEL"
                 operation="process" variable="input"
                 createInstance="yes"/>
        
        <!-- Generate reply to synchronous request -->
        <bpel:sequence name="SequenceGetProduitByRef">
            <bpel:assign validate="no" name="AssignProduit"></bpel:assign>
            <bpel:invoke name="InvokeWSStockGetProduit"></bpel:invoke>
            <bpel:assign validate="no" name="AssignRetourProduit"></bpel:assign>
            <bpel:if name="If">
                <bpel:else></bpel:else>
            </bpel:if>
        </bpel:sequence>
        <bpel:reply name="replyOutput" 
               partnerLink="client"
               portType="tns:StockBPEL"
               operation="process" 
               variable="output"
               />
    </bpel:sequence>
</bpel:process>

